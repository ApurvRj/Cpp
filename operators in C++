#include <iostream>
using namespace std;

int main() {
  // arithmetic operators
    int a, b;
    a = 7;
    b = 2;

    cout << "a + b = " << (a + b) << endl;
    cout << "a - b = " << (a - b) << endl;
    cout << "a * b = " << (a * b) << endl;
    cout << "a / b = " << (a / b) << endl;
    cout << "a % b = " << (a % b) << endl;

  //increment decrement
    int c = 10, d = 100, result_c, result_d;

    result_c = ++c;
    cout << "result_c = " << result_c << endl;
   
    result_d = --d;
    cout << "result_d = " << result_d << endl;
  
  // assignment operators
    int e, f;
    e = 2;
    f = 7;

    cout << "e = " << e << endl;
    cout << "f = " << f << endl;
    cout << "\nAfter e += f;" << endl;
    e += f;
    cout << "e = " << e << endl;

  // relational operators logical operators
    int g, h;
    g = 3;
    h = 5;
    bool result;

    result = (a == b);   
    cout << "3 == 5 is " << result << endl;

    result = (a != b);
    cout << "3 != 5 is " << result << endl;

    result = a > b;   
    cout << "3 > 5 is " << result << endl;

    result = a < b;   
    cout << "3 < 5 is " << result << endl;

    result = a >= b;  
    cout << "3 >= 5 is " << result << endl;

    result = a <= b;  
    cout << "3 <= 5 is " << result << endl;

    result = (3 != 5) && (3 < 5); 
    cout << "(3 != 5) && (3 < 5) is " << result << endl;

    result = (3 == 5) && (3 < 5); 
    cout << "(3 == 5) && (3 < 5) is " << result << endl;

    result = (3 == 5) && (3 > 5); 
    cout << "(3 == 5) && (3 > 5) is " << result << endl;

    result = (3 != 5) || (3 < 5); 
    cout << "(3 != 5) || (3 < 5) is " << result << endl;

    result = (3 != 5) || (3 > 5); 
    cout << "(3 != 5) || (3 > 5) is " << result << endl;

    result = (3 == 5) || (3 > 5); 
    cout << "(3 == 5) || (3 > 5) is " << result << endl;

    result = !(5 == 2);    
    cout << "!(5 == 2) is " << result << endl;

    result = !(5 == 5);    
    cout << "!(5 == 5) is " << result << endl;
    
    reyurn 0;

}
