#include <iostream>
using namespace std;
int main() {
  int a = 321;
  cout<<a << endl;

  char b = 'v';
  cout<< b << endl;

  bool bl = true;
  cout << bl << endl;

  float f = 1.2;
  cout << f << endl;

  double d = 1.23432;
  cout << d << endl;

  int size = sizeof(a);
  cout << size << endl;

  int size1 = sizeof(d);
  cout << size1 << endl;
}
