#include <iostream>
using namespace std;
int main() {
  int a = 321;
  cout<<a << endl;

  char b = 'v';
  cout<< b << endl;

  bool bl = true;
  cout << bl << endl;

  float f = 1.2;
  cout << f << endl;

  double d = 1.23432;
  cout << d << endl;

  int size = sizeof(a);
  cout << size << endl;

  int size1 = sizeof(d);
  cout << size1 << endl;
  
  int e = 'e';
  cout << e << endl;

  char ch = 98;
  cout << ch << endl;

  char ch1 = 123456;
  cout << ch1 << endl;
  
  unsigned int g = 112;
  cout << g << endl;

  unsigned int h = -112;
  cout << h << endl;
}
